{
    "name": "fidelite/laravel-calendly-api",
    "description": "PHP Calendly API (v2) SDK for Laravel",
    "keywords": [
        "laravel",
        "calendly",
        "api",
        "rest",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeni Nurkov",
            "email": "evgeni.nurkov@gmail.com"
        }
    ],
    "require": {
        "php": "^8",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Fidelite\\Calendly\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fidelite\\Calendly\\CalendlyServiceProvider"
            ],
            "aliases": {
                "Calendly": "Fidelite\\Calendly\\Calendly"
            }
        }
    },
    "minimum-stability": "dev"
}
